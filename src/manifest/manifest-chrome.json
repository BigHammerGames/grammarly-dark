{
	"manifest_version": 2,
	"name": "Grammarly Dark",
	"description": "A dark theme for Grammarly documents.",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": ["https://app.grammarly.com/ddocs/*"],
			"css": ["index.css"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
